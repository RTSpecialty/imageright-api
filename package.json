    "chai": "^4.2.0",
    "concat-stream": "^2.0.0",
    "del": "^3.0.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^3.0.1",
